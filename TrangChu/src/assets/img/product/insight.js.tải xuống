function ants_isIE(){var e=navigator.userAgent.toLowerCase();return-1!=e.indexOf("msie")?parseInt(e.split("msie")[1]):!1}function _ants_b64ToUint6(e){return 64<e&&91>e?e-65:96<e&&123>e?e-71:47<e&&58>e?e+4:43===e?62:47===e?63:0}function _ants_base64DecToArr(e,t){try{for(var n=e.replace(/[^A-Za-z0-9\+\/]/g,""),r=n.length,i=t?Math.ceil((3*r+1>>2)/t)*t:3*r+1>>2,s=new Uint8Array(i),o,u,a=0,f=0,l=0;l<r;l++)if(u=l&3,a|=_ants_b64ToUint6(n.charCodeAt(l))<<6*(3-u),3===u||1===r-l){for(o=0;3>o&&f<i;o++,f++)s[f]=a>>>(16>>>o&24)&255;a=0}return s}catch(c){return""}}function uint6ToB64(e){return 26>e?e+65:52>e?e+71:62>e?e-4:62===e?43:63===e?47:65}function _ants_base64EncArr(e){for(var t=2,n="",r=e.length,i=0,s=0;s<r;s++)if(t=s%3,0<s&&0===4*s/3%76&&(n+="\r\n"),i|=e[s]<<(16>>>t&24),2===t||1===e.length-s)n+=String.fromCharCode(uint6ToB64(i>>>18&63),uint6ToB64(i>>>12&63),uint6ToB64(i>>>6&63),uint6ToB64(i&63)),i=0;return n.substr(0,n.length-2+t)+(2===t?"":1===t?"=":"==")}function _ants_UTF8ArrToStr(e){for(var t="",n,r=e.length,i=0;i<r;i++)n=e[i],t+=String.fromCharCode(251<n&&254>n&&i+5<r?1073741824*(n-252)+(e[++i]-128<<24)+(e[++i]-128<<18)+(e[++i]-128<<12)+(e[++i]-128<<6)+e[++i]-128:247<n&&252>n&&i+4<r?(n-248<<24)+(e[++i]-128<<18)+(e[++i]-128<<12)+(e[++i]-128<<6)+e[++i]-128:239<n&&248>n&&i+3<r?(n-240<<18)+(e[++i]-128<<12)+(e[++i]-128<<6)+e[++i]-128:223<n&&240>n&&i+2<r?(n-224<<12)+(e[++i]-128<<6)+e[++i]-128:191<n&&224>n&&i+1<r?(n-192<<6)+e[++i]-128:n);return t}function _ants_strToUTF8Arr(e){var t,n;t=e.length;for(var r=0,i=0;i<t;i++)n=e.charCodeAt(i),r+=128>n?1:2048>n?2:65536>n?3:2097152>n?4:67108864>n?5:6;t=new Uint8Array(r);for(var s=i=0;i<r;s++)n=e.charCodeAt(s),128>n?t[i++]=n:(2048>n?t[i++]=192+(n>>>6):(65536>n?t[i++]=224+(n>>>12):(2097152>n?t[i++]=240+(n>>>18):(67108864>n?t[i++]=248+(n>>>24):(t[i++]=252+(n>>>30),t[i++]=128+(n>>>24&63)),t[i++]=128+(n>>>18&63)),t[i++]=128+(n>>>12&63)),t[i++]=128+(n>>>6&63)),t[i++]=128+(n&63));return t}function ants_encryptBase64(e){if(9==ants_isIE())r